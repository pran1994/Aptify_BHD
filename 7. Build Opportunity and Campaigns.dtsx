                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select distinct N'Premium Membership' as RecordTypeId, o.id as Aptify_Id__c, o.Name,AccountManager,PersonID,CompanyID,CampaignID,Type,Status,Stage,
Source,LeadQuality__afl,ExpectedDecisionDate,ActualDecisionDate,LostTo,
Description = CONCAT_WS(char(10) + char(13),'Historical Aptify values:','Stage:' + Stage,'Status:' + Status,'Type:' + Type,'Source:' + Source,'Comments:' + o.Comments),
