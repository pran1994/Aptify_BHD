--,Source,LostTo,ExpectedDecisionDate,ActualDecisionDate,o.Comments,
--LeadQuality__afl,d.*
from Aptify.vwOpportunities o
inner join Aptify.vwOpportunityDetails d on
	o.ID = d.OpportunityID
inner join import.[APTIFY AFL - Product Filter] p on
	d.ProductID = p.id
where p.[Retain Premium (Y N)] = 'Y'
or p.[Retain Events (Y N)] = 'Y'</property>
